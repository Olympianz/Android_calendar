<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chong.sukuruintern"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.chong.sukuruintern.Register"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.chong.sukuruintern.SignUp"
            android:label="@string/app_signUp" >
            <intent-filter>
                <action android:name="fordham.chong.signUp" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.chong.sukuruintern.CreateCalendar"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="fordham.chong.createCalendar" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.chong.sukuruintern.ImportCalendar"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="fordham.chong.importCalendar" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.chong.sukuruintern.ShowCalendar"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="fordham.chong.showCalendar" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
